@prefix schema: <http://schema.org/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<https://fornpunkt.se/observationstyper#fjarr>
  a rdfs:Class ;
  rdfs:label "Fjärrobservation"@sv ;
  rdfs:comment "Observation gjord på distans utan ett besök i fält."@sv ;
  rdfs:subClassOf schema:Event .

<https://fornpunkt.se/observationstyper#falt>
  a rdfs:Class ;
  rdfs:label "Fältobservation"@sv ;
  rdfs:comment "Observation gjord genom ett eller flera besök i fält."@sv ;
  rdfs:subClassOf schema:Event .

<https://fornpunkt.se/observationstyper#maskin>
  a rdfs:Class ;
  rdfs:label "Maskinobservation"@sv ;
  rdfs:comment "Observation gjord av ett dator program genom dataanalys."@sv ;
  rdfs:subClassOf schema:Event .
